{
  "extends": ["eslint:recommended"],
  "plugins": [],
  "parserOptions": {},
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "jquery": true,
    "commonjs": true
  },
  "globals": {},
  "rules": {
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "no-trailing-spaces": ["error"],
    "quotes": [2, "single"],
    "keyword-spacing": ["error"],
    "camelcase": "error",
    "brace-style": "error",
    "curly": "error",
    "eqeqeq": "error",
    "no-with": "error",
    "no-eval": "error",
    "no-new-wrappers": "error",
    "no-array-constructor": "error",
    "no-new-object": "error",
    "new-cap": "error",
    "consistent-this": ["error", "that"],
    "no-console": "warn",
    "no-unused-vars": "warn"
  }
}
